 @page
@model admin_panel_razor.Pages.ShopCoins.DeleteModel
@{
    ViewData["Title"] = "Delete Shop Coin";
}

<h1>Delete Shop Coin</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Shop Coin</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Id
        </dt>
        <dd class="col-sm-10">
            @Model.ShopCoin.Id
        </dd>
        <dt class="col-sm-2">
            Value
        </dt>
        <dd class="col-sm-10">
            @Model.ShopCoin.Value
        </dd>
    </dl>

    <form method="post">
        <input type="hidden" asp-for="ShopCoin.Id" />
        <button type="submit" class="btn btn-danger">Delete</button>
        <a asp-page="Index" class="btn btn-secondary">Cancel</a>
    </form>
</div>
